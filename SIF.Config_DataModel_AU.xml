		<!-- NN 20221216 dmToJsonSchemaCreateStrict and dmToJsonSchemaUpdateStrict comply with OpenAPI 3.0
		dmToJsonSchemaCreate and dmToJsonSchemaUpdate generate full JSON Schema, and are thus only
		compliant with OpenAPI 3.1. (They only differ in their parameters currently.)  -->
		
		<xsltTransformation>
			<xsltInputFile>98_Xslt\dmToJsonSchemaCreateStrict.xsl</xsltInputFile>
			<xsltOutputDir>JSON</xsltOutputDir>
			<xsltOutputFile>Full/jsonSchema_OpenAPI3.0_Create_AU.yaml</xsltOutputFile>
			<yamlToJson>true</yamlToJson>
		</xsltTransformation>
		
		<xsltTransformation>
			<xsltInputFile>98_Xslt\dmToJsonSchemaUpdateStrict.xsl</xsltInputFile>
			<xsltOutputDir>JSON</xsltOutputDir>
			<xsltOutputFile>Full/jsonSchema_OpenAPI3.0_Update_AU.yaml</xsltOutputFile>
			<yamlToJson>true</yamlToJson>
		</xsltTransformation>

		<xsltTransformation>
			<xsltInputFile>98_Xslt\dmToOpenAPI30.xsl</xsltInputFile>
			<xsltParamList>
				<xsltParam>
					<xsltName>includeAllHeaders</xsltName>
					<xsltValue>true</xsltValue>
				</xsltParam>
				<xsltParam>
					<xsltName>includeAdminDirectives</xsltName>
					<xsltValue>true</xsltValue>
				</xsltParam>
				<xsltParam>
					<xsltName>omitVersionInExamplesFileName</xsltName>
					<xsltValue>true</xsltValue>
				</xsltParam>
			</xsltParamList>
			<xsltOutputDir>JSON</xsltOutputDir>
			<xsltOutputFile>Full/OpenAPI3.0_AU.yaml</xsltOutputFile>
			<yamlToJson>true</yamlToJson>
		</xsltTransformation>	

		
