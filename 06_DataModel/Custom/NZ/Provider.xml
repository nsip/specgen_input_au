<DataObject name="Provider"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
			xmlns:json="http://james.newtonking.com/projects/json"
			
            xmlns="http://sifassociation.org/SpecGen">
  <Key>@RefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>Provider</Element>
	<privacyRating>Low</privacyRating>
    <Description>A recognised provider of education services in the New Zealand
	education system.  Providers may operate in the Early Childhood, Compulsory Schooling or Tertiary sectors.
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
	<privacyRating>Low</privacyRating>
    <Description>
      <p>The SIF GUID used by systems to identify the provider.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item> <!-- @RefId -->
  <Item>
    <Element>LocalId</Element>
    <Characteristics>O</Characteristics>
	<privacyRating>Low</privacyRating>
    <Description>
      <p>The Ministry of Education number used by the education sector to identify the provider.</p>
    </Description>
    <Type ref="CommonTypes" name="LocalIdType"></Type>
  </Item> <!-- LocalId -->
  <Item>
	  <Element>Authority</Element>
	  <Characteristics>M</Characteristics>
	  <privacyRating>Low</privacyRating>
	  <Description>
		  <p>A code denoting the authority by which the Provider is authorised to
		  provide education services.
		  </p>
	  </Description>
	  <Type ref="CodeSets" name="NZCodeSetsProviderAuthorityType"/>
  </Item> <!-- Authority -->
  <Item>
    <Element>Organisation</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The generic organisation details about the provider</p>
    </Description>
    <Type ref="CommonTypes" name="OrganisationType"></Type>
  </Item> <!-- Organisation -->
  <Item>
	  <Element>SchoolService</Element>
	  <Characteristics>O</Characteristics>
	  <privacyRating>Low</privacyRating>
	  <Description>
		  <p>Only present if the Provider is a provider of school education services.
		  Contains properties that are school-specific. </p>
	  </Description>
	  <Type ref="CommonTypes" name="SchoolServiceType"/>
  </Item> <!-- SchoolService -->
    
