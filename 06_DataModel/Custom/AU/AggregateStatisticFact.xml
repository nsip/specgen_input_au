<DataObject name="AggregateStatisticFact">
  <EventsReported>true</EventsReported>
  <Key>@RefId</Key>
  <Item>
    <Element>AggregateStatisticFact</Element>
    <Description>
      <p>This object is the actual value of a statistical measurement. It is always associated with one statistic and one or more characteristics.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description></Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Element>AggregateStatisticInfoRefId</Element>
    <Characteristics>M</Characteristics>
    <Description>
      Reference to statistic defined by <code>AggregateStatisticInfo</code>.
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>Characteristics</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>
        Reference to one or more <code>AggregateCharacteristicInfo</code> objects.
      </p>
    </Description>
    <List mode="List">
    </List>
  </Item>
  <Item>
    <Element>/AggregateCharacteristicInfoRefId</Element>
    <Characteristics>MR</Characteristics>
    <Description>
      <p>
        Reference to an <code>AggregateCharacteristicInfo</code> object.
      </p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>Excluded</Element>
    <Characteristics>O</Characteristics>
    <Description></Description>
    <Values>
      <Value>
        <Code>Yes</Code>
      </Value>
      <Value>
        <Code>No</Code>
      </Value>
    </Values>
  </Item>
  <Item>
    <Element>Value</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Value of the fact.</p>
    </Description>
    <Type name="xs:decimal"></Type>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_Metadata"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
  </Item>
  <xhtml:Example xmlns="">
    <AggregateStatisticFact RefId="3A4E2822C696433B812B49DEAE557E41">
      <AggregateStatisticInfoRefId>91A4209F7F4F4F4B9DC8D21CEFD1DC2F</AggregateStatisticInfoRefId>
      <Characteristics>
        <AggregateCharacteristicInfoRefId>0B972D8644EE4D7994F5FF1C3131772D</AggregateCharacteristicInfoRefId>
      </Characteristics>
      <Excluded>No</Excluded>
      <Value>27</Value>
    </AggregateStatisticFact>
  </xhtml:Example>
</DataObject>