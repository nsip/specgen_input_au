<DataObject name="NAPTestlet"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
	
            xmlns="http://sifassociation.org/SpecGen">
  <Key>@RefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>NAPTestlet</Element>
	<privacyRating>Low</privacyRating>
    <Description>
      <p>This object represents a studentâ€™s attempt to undergo NAP assessment in a particular test.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
	<privacyRating>Low</privacyRating>
    <Description>
      <p>The GUID that uniquely identifies an instance of the object.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  
   <Item>
    <Element>NAPTestRefId</Element>
    <Characteristics>O</Characteristics>
	<privacyRating>Low</privacyRating>
    <Description>
      <p>The GUID for the test that the student is sitting. May be linking to prior codeframe download.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  
    <Item>
		<Element>NAPTestLocalId</Element>
		<Characteristics>M</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>Local identifier of NAP test.</p>
		</Description>
		<Type ref="CommonTypes" name="LocalIdType"/> 
  </Item>

   <Item>
    <Element>TestletContent</Element>
    <Characteristics>M</Characteristics>
	<privacyRating>Low</privacyRating>
    <Type ref="CommonTypes" name="NAPTestletContentType"></Type>
  </Item>
  
   <Item>
		<Element>TestItemList</Element>
		<Characteristics>M</Characteristics>
		<privacyRating>Low</privacyRating>
		<Description>
			<p>List of test items contained in the testlet.</p>
		</Description>
		<Type ref="CommonTypes" name="NAPTestItemListType"/> 
  </Item>  	

  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
	<privacyRating>Low</privacyRating>
    <Type ref="CommonTypes" name="SIF_MetadataType"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
	<privacyRating>Low</privacyRating>
    <Type ref="CommonTypes" name="SIF_ExtendedElementsType"></Type>
  </Item>

