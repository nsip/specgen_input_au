<DataObject name="SchoolInfo">
  <Key>@RefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>SchoolInfo</Element>
    <Description>
      <p>This object contains information about the school or campus.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The GUID that identifies this school.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Element>LocalId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The locally-assigned identifier for this school.</p>
    </Description>
    <Type ref="DataModel" name="LocalIdType"></Type>
  </Item>
  <Item>
    <Element>StateProvinceId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The state-assigned identifier for this school.</p>
    </Description>
    <Type ref="DataModel" name="StateProvinceIdType"></Type>
  </Item>
  <Item>
    <Element>CommonwealthId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Commonwealth identifier for this school/campus.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
   <Item>
    <Element>ACARAId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>ACARA identifier for this school/campus.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Element>OtherIdList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Lists all "other" identifiers associated with the school.</p>
    </Description>
    <List mode="List">
    </List>
  </Item>
  <Item>
    <Element>/OtherId</Element>
    <Characteristics>MR</Characteristics>
    <Description>
      <p>Lists an "other" identifier associated with the school.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Attribute>Type</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Code that defines the type of this other ID.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Element>SchoolName</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Name of school/campus.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Element>LEAInfoRefId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The ID (GUID) that references the school district of which this school is a member.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Element>OtherLEA</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The ID (GUID) of another related education agency, such as a regional service agency.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Attribute>SIF_RefObject</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The name of the object reference. </p>
    </Description>
    <Values>
      <Value>
        <Code>LEAInfo</Code>
      </Value>
    </Values>
  </Item>
  <Item>
    <Element>SchoolDistrict</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The school district of which this school/campus is a member. Equivalent to DEECD Region.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>

  <Item>
    <Element>SchoolDistrictLocalId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The school district Local ID. This should be the same as the LocalId in the LEAInfo if LEAInfo details are supplied.</p>
    </Description>
    <Type ref="DataModel" name="LocalIdType"></Type>
  </Item>
 
  
  <Item>
    <Element>SchoolType</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>An indication of the level of the educational institution.</p>
    </Description>
    <Type ref='CodeSets' name='AUCodeSetsSchoolLevelType'/>
  </Item>
  <Item>
    <Element>SchoolFocusList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>List containing the education focus codes of the campus/school.</p>
    </Description>
    <List mode="List">
    </List>
  </Item>
  <Item>
    <Element>/SchoolFocus</Element>
    <Characteristics>MR</Characteristics>
    <!--xs:maxOccurs value="6"/-->
    <Description>
      <p>The type of educational institution as classified by its focus.</p>
    </Description>
    <Type ref='CodeSets' name='AUCodeSetsSchoolFocusCodeType'/>
  </Item>
  <Item>
    <Element>SchoolURL</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>URL for the school.</p>
    </Description>
    <Type ref="DataModel" name="SchoolURLType"></Type>
  </Item>
  
  <Item>
    <Element>SchoolEmailList</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="EmailListType"></Type>
    <Description>
      <p>List of contact emails for the school.</p>
    </Description>
  </Item>
  
  <Item>
    <Element>PrincipalInfo</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>Information about the campus or school principal.</p>
    </Description>
    <Type ref="DataModel" name="PrincipalInfoType"></Type>
  </Item>
  
  <Item>
    <Element>SchoolContactList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>A list of contact persons associated with a school.</p>
    </Description>
    <Type ref="DataModel" name="SchoolContactListType"></Type>
  </Item>
  <Item>
    <Element>AddressList</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="AddressListType"></Type>
    <Description>
      <p>The school's addresses.</p>
    </Description>
  </Item>
  <Item>
    <Element>PhoneNumberList</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="PhoneNumberListType"></Type>
    <Description>
      <p>The school's phone numbers.</p>
    </Description>
  </Item>
  <Item>
    <Element>SessionType</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Code that specifies the session type.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsSessionTypeType"></Type>
  </Item>
  
  <Item>
    <Element>YearLevels</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>List of year levels offered by the school.</p>
    </Description>
    <Type ref="DataModel" name="YearLevelsType"></Type>
  </Item>
  
 
     
  <Item>
    <Element>ARIA</Element>
    <Characteristics>O</Characteristics>
    <Description>Accessibility/Remoteness Indicator of Australia.</Description>
    <Type name='xs:decimal'/>
  </Item>
  <Item>
    <Element>OperationalStatus</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Operational condition of a school.</p>
    </Description>
     <Type ref="DataModel" name="OperationalStatusType"></Type>
  </Item>
  <Item>
    <Element>FederalElectorate</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Australian Federal Electorate.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsFederalElectorateType"></Type>
  </Item>
  <Item>
    <Element>Campus</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Optional campus details.</p>
    </Description>
  </Item>
  <Item>
    <Element>/ParentSchoolId</Element>
    <Characteristics>C</Characteristics>
    <Description>
      <p>Parent School Id needs to be supplied if this Campus is not the Admin Campus.  This number should be the state-assigned identifier for this school or the locally-assigned identifier for this school.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Element>/SchoolCampusId</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Specific Campus Id  - This may be the same as the School Local Id.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Element>/CampusType</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Type of campus.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsSchoolLevelType"></Type>
  </Item>
  <Item>
    <Element>/AdminStatus</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Is this campus the Admin Campus?</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsYesOrNoCategoryType"></Type>
  </Item>
  <Item>
    <Element>SchoolSector</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>(Government, or Non-Government).</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsSchoolSectorCodeType"></Type>
  </Item>
  <Item>
    <Element>IndependentSchool</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Indicator as to whether school is an independent school (as opposed to government or catholic school).</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsYesOrNoCategoryType"></Type>
  </Item>
  <Item>
    <Element>NonGovSystemicStatus</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Used to clarify a non-government school's systemic status. Either Systemic or Non-Systemic.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsSystemicStatusType"></Type>
  </Item>
  <Item>
    <Element>System</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>System if a systemic school.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsSchoolSystemType"></Type>
  </Item>
  <Item>
    <Element>ReligiousAffiliation</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Religious affiliation (if any).</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsAustralianStandardClassificationOfReligiousGroupsASCRGType"></Type>
  </Item>
  <Item>
    <Element>SchoolGeographicLocation</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>School Location from MCEETCYA.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsSchoolLocationType"></Type>
  </Item>
  <Item>
    <Element>LocalGovernmentArea</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>LocalGovernmentArea that that school/campus is located in.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Element>JurisdictionLowerHouse</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Lower House area  that the school/campus  belongs to.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Element>SLA</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Statistical Local Area that the school/campus belongs to.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsAustralianStandardGeographicalClassificationASGCType"></Type>
  </Item>
  <Item>
    <Element>SchoolCoEdStatus</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Gender of student population.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsSchoolCoEdStatusType"></Type>
  </Item>
  <Item>
    <Element>BoardingSchoolStatus</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Is this school a Boarding School?</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsYesOrNoCategoryType"></Type>
  </Item>
  
   <Item>
    <Element>YearLevelEnrollmentList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Container for number of students enrolled per year level.</p>
    </Description>
    <List mode="List">
    </List>
  </Item>
  <Item>
    <Element>/YearLevelEnrollment</Element>
    <Characteristics>MR</Characteristics>
    <Description>
      <p>Containter for Year Level of Enrolments.</p>
    </Description>
  </Item>
  <Item>
    <Element>//Year</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Year level for which students are counted.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsYearLevelCodeType"></Type>
  </Item>
   <Item>
    <Element>//Enrollment</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Number of students enrolled in the year level.</p>
    </Description>
     <Type name="xs:normalizedString"></Type>
  </Item>
  
   <Item>
    <Element>TotalEnrollments</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Optional School/Campus enrolments.</p>
    </Description>
  </Item>
  <Item>
    <Element>/Girls</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Number of enrolled Female students.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Element>/Boys</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Number of enrolled Male students.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
   <Item>
    <Element>/TotalStudents</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Total number of enrolled students.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>

  <Item>
    <Element>Entity_Open</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Opening date of entity.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>
  <Item>
    <Element>Entity_Close</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Closing date of entity.  This element is allowed to be omitted and/or null.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>
  <Item>
    <Element>SchoolGroupList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p></p>
    </Description>
    <List mode='List'/>
  </Item>
  <Item>
    <Element>/SchoolGroup</Element>
    <Characteristics>MR</Characteristics>
    <Description>
      <p>Repeatable element containing a local identifier to identify a loosely connected group of schools. eg. YarraValley Cluster</p>
    </Description>
    <Type ref="DataModel" name="LocalIdType"></Type>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_MetadataType"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElementsType"></Type>
  </Item>
