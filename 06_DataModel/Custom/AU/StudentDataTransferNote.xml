<DataObject name="StudentDataTransferNote"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:xi="http://www.w3.org/2001/XInclude"
			xmlns:xhtml="http://www.w3.org/1999/xhtml"
	
            xmlns="http://sifassociation.org/SpecGen">

  <Key>@RefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>StudentDataTransferNote</Element>
	<privacyRating>High</privacyRating>
    <Description>
      <p>Object for Student Data Transfer Note, to communicate student movements between schools via a national Student Data Transfer Exchange.</p>
     </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
	<privacyRating>High</privacyRating>
    <Description>
      <p>GUID for this object. The application that owns this object is responsible for generating this unique Id.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  
  <Item>
	  <Element>Name</Element>
	  <Characteristics>M</Characteristics>
	  <privacyRating>High</privacyRating>
	  <Description>The name of the person. Note: Type attribute value of LGL must be used here.</Description>
	  <Type ref="CommonTypes" name="NameOfRecordType"></Type>
  </Item>
  
  <Item>
	  <Element>Gender</Element>
	  <Characteristics>M</Characteristics>
	  <privacyRating>High</privacyRating>
	  <Description>The distinction "male"/"female"/"intersex"/"self-described", as reported by the person.</Description>
	  <Type ref="CodeSets" name="AUCodeSetsSexCodeType"/>
  </Item>
  
  <Item>
	  <Element>BirthDate</Element>
	  <Characteristics>M</Characteristics>
	  <privacyRating>High</privacyRating>
	  <Description>The person's date of birth.</Description>
	  <Type ref="CommonTypes" name="BirthDateType"/>
  </Item>
  
  <Item>
	  <Element>PlaceOfBirth</Element>
	  <Characteristics>O</Characteristics>
	  <privacyRating>High</privacyRating>
	  <Description>The person's place of birth -- such as village, town, city, etc.</Description>
	  <Type name="xs:normalizedString"/>
  </Item>
  
 	<Item>
		<Element>StateOfBirth</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>The person's state of birth.</p>
		</Description>
		<Type ref="CommonTypes" name="StateProvinceType"/>
	</Item>
	<Item>
		<Element>CountryOfBirth</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>The person's country of birth.</p>
		</Description>
		<Type ref="CommonTypes" name="CountryType"/>
	</Item>
  
 	<Item>
		<Element>CountriesOfCitizenship</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>The person's countries of citizenship.</p>
		</Description>
		<Type ref="CommonTypes" name="CountryListType"/>
	</Item>

	<Item>
		<Element>DepartureSchool</Element>
		<Characteristics>M</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>The school that the student has departed from.</p>
		</Description>
		<Type ref="CommonTypes" name="DepartureSchoolType"/>
	</Item>

 	<Item>
		<Element>PreviousSchoolList</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Previous schools that the student has attended.</p>
		</Description>
		<Type ref="CommonTypes" name="PreviousSchoolListType"/>
	</Item>

  	<Item>
		<Element>NAPLANScoreList</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Scores student has attained in most recent NAPLAN assessment.</p>
		</Description>
		<Type ref="CommonTypes" name="NAPLANScoreListType"/>
	</Item>
	<!-- Go back and revisit obligations on DomainScore elements in light of feedback on what scores can be published -->

 	<Item>
		<Element>NCCDList</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>List of applicable disability categories for the student from the Nationally Consistent Collection of Data.</p>
		</Description>
		<Type ref="CommonTypes" name="NCCDListType"/>
	</Item>

	<Item>
		<Element>AtRisk</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Student is at risk, and follow-up discussion with departure school is requested.</p>
		</Description>
	  <Type name="xs:boolean"/>
	</Item>

 	<Item>
		<Element>AttendanceFlag</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Student has attendance issues, and follow-up discussion with departure school is requested.</p>
		</Description>
	  <Type name="xs:boolean"/>
	</Item>

	<Item>
		<Element>NationalUniqueStudentIdentifier</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>The Commonwealth-assigned National Unique Student Identifier (USI) for this student.</p>
		</Description>
	  <Type name="xs:token"/>
	</Item>

	<Item>
		<Element>YearLevel</Element>
		<Characteristics>M</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Year or academic level of student.</p>
		</Description>
		<Type ref="CommonTypes" name="YearLevelType"/>
	</Item>

	<Item>
		<Element>IndigenousStatus</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Whether or not the person identifies themselves as being of Aboriginal and/or Torres Strait Islander descent.</p>
		</Description>
	  <Type ref="CodeSets" name="AUCodeSetsIndigenousStatusType"/>
	</Item>

	<Item>
		<Element>LBOTE</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Language Background Other Than English.</p>
		</Description>
		<Type ref="CommonTypes" name="LBOTEAndLanguageType"/>
	</Item>

	<Item>
		<Element>VisaStatus</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Visa status of student.</p>
		</Description>
		<Type ref="CommonTypes" name="VisaSubClassType"/>
	</Item>

	<Item>
		<Element>OtherNames</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Other or previous names of student.</p>
		</Description>
		<Type ref="CommonTypes" name="OtherNamesType"/>
	</Item>

	<Item>
		<Element>EducationalAssessmentList</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>List of relevant educational assesssments the student has undertaken, other than NAPLAN.</p>
		</Description>
		<Type ref="CommonTypes" name="EducationalAssessmentListType"/>
	</Item>

	<Item>
		<Element>StudentGradeList</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>List of A-E Grades most recently received by student.</p>
		</Description>
		<Type ref="CommonTypes" name="STDNGradeListType"/>		
	</Item>
	
	<Item>
		<Element>DisciplinaryAbsences</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Flag for disciplinary absences, indicating request for follow-up discussion.</p>
		</Description>
		<Type ref="CommonTypes" name="FlagAndDetailsType"/>		
	</Item>

	<Item>
		<Element>Health</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Flag for health issues, indicating request for follow-up discussion.</p>
		</Description>
		<Type ref="CommonTypes" name="FlagAndDetailsType"/>		
	</Item>

	<Item>
		<Element>Talents</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Flag for talents, indicating request for follow-up discussion.</p>
		</Description>
		<Type ref="CommonTypes" name="FlagAndDetailsType"/>		
	</Item>

	<Item>
		<Element>EducationPlan</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Flag for an education plan, indicating request for follow-up discussion.</p>
		</Description>
		<Type ref="CommonTypes" name="FlagAndDetailsType"/>		
	</Item>

	<Item>
		<Element>Behaviour</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Flag for behaviour issues, indicating request for follow-up discussion.</p>
		</Description>
		<Type ref="CommonTypes" name="FlagAndDetailsType"/>		
	</Item>

	<Item>
		<Element>CareerGuidance</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Flag for career guidance issues, indicating request for follow-up discussion.</p>
		</Description>
		<Type ref="CommonTypes" name="FlagAndDetailsType"/>		
	</Item>

	<Item>
		<Element>YoungCarer</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Flag for young carer issues, indicating request for follow-up discussion.</p>
		</Description>
		<Type ref="CommonTypes" name="FlagAndDetailsType"/>		
	</Item>

	<Item>
		<Element>Counselling</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Flag for counselling issues, indicating request for follow-up discussion.</p>
		</Description>
		<Type ref="CommonTypes" name="FlagAndDetailsType"/>		
	</Item>

	<Item>
		<Element>Acceleration</Element>
		<Characteristics>O</Characteristics>
	    <privacyRating>High</privacyRating>
		<Description>
			<p>Flag for acceleration issues, indicating request for follow-up discussion.</p>
		</Description>
		<Type ref="CommonTypes" name="FlagAndDetailsType"/>		
	</Item>

<Item>   
    <Element>LocalCodeList</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>High</privacyRating>
    <Type ref="CommonTypes" name="LocalCodeListType"></Type>
  </Item>
  
  <Item>   
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>High</privacyRating>
    <Type ref="CommonTypes" name="SIF_MetadataType"></Type>
  </Item>
  
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <privacyRating>High</privacyRating>
    <Type ref="CommonTypes" name="SIF_ExtendedElementsType"></Type>
  </Item>
  
