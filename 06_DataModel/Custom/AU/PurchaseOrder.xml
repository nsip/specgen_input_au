<DataObject name="PurchaseOrder">
  <Key>@RefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>PurchaseOrder</Element>
    <Description>
      <p>
        This object provides information down to the line item level for purchase orders.</p>
     </Description>
  </Item>
  <Item>

    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Transaction GUID for this purchasing activity. The application that owns this object and is responsible for generating this unique Id.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>

  <Item>
    <Element>FormNumber</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Purchase order number.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  
  <Item>
    <Element>VendorInfoRefId</Element>    
    <Characteristics>M</Characteristics>
    <Description>
      <p>This object references the VendorInfo object being billed.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  
  <Item>
    <Element>ChargedLocationInfoRefId</Element>    
    <Characteristics>O</Characteristics>
    <Description>
      <p>The location Id for this purchasing activity.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  
  <Item>
    <Element>EmployeePersonalRefId</Element>    
    <Characteristics>O</Characteristics>
    <Description>
      <p>The employee Id for the originator of this purchasing activity.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  
  <Item>
    <Element>PurchasingItems</Element>    
    <Characteristics>M</Characteristics>
    <Description>
      <p></p>
    </Description>
    <List mode="List"></List>
  </Item>
  

  <Item>
    <Element>/PurchasingItem</Element>    
    <Characteristics>MR</Characteristics>
    <Description>
      <p>Contains information about the item being purchased.</p>
    </Description>
  </Item>
  
  <Item>
    <Element>//ItemNumber</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Vendor item number.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>

  <Item>
    <Element>//ItemDescription</Element>   
    <Characteristics>M</Characteristics>
    <Description>
      <p>Description of the item.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  
  <Item>
    <Element>//Quantity</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p>Quantity ordered.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  
  <Item>
    <Element>//UnitCost</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p>Unit cost of the item.</p>
    </Description>
    <Type ref="CommonTypes" name="MonetaryAmountType"></Type>
  </Item>
  
  <Item>
    <Element>//TotalCost</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p>Total cost of the item.</p>
    </Description>
    <Type ref="CommonTypes" name="MonetaryAmountType"></Type>
  </Item>
  
  <Item>
    <Element>//QuantityDelivered</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p>Quantity delivered to date.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  
  <Item>
    <Element>//CancelledOrder</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p>Indicates that the line item in the purchase order is a cancellation of a previous order, to prevent it from remaining outstanding. For example, if 10 widgets have been ordered, and only 7 widgets have been delivered, a new purchase order can be issued for 3 widgets; enabling this flag indicates that the order for the remaining 3 widgets is to be cancelled.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsYesOrNoCategoryType"></Type>
  </Item>
  
  <Item>
    <Element>//TaxRate</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p>Rate of tax included in the billed amount for the item.</p>
    </Description>
    <Type name="xs:decimal"></Type>
  </Item>
  
  <Item>
    <Element>//ExpenseAccounts</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p></p>
    </Description>
    <List mode="List"></List>
  </Item>
  
  <Item>
    <Element>///ExpenseAccount</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p>Account that is to be charged for the item.</p>
    </Description>    
  </Item>
  
  <Item>
    <Element>////AccountCode</Element>   
    <Characteristics>M</Characteristics>
    <Description>
      <p>Account code charged to this item.</p>
    </Description>    
    <Type name="xs:token"></Type>
  </Item>
  
  <Item>
    <Element>////Amount</Element>   
    <Characteristics>M</Characteristics>
    <Description>
      <p>Amount charged to this account.</p>
    </Description>    
    <Type ref="CommonTypes" name="MonetaryAmountType"></Type>
  </Item>
  
  <Item>
    <Element>////FinancialAccountRefId</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p>Account being charged.</p>
    </Description>    
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  
  <Item>
    <Element>////AccountingPeriod</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p>Accounting period for the charge.</p>
    </Description>    
    <Type ref="DataModel" name="LocalIdType"></Type>
  </Item>
  
  <Item>
    <Element>CreationDate</Element>    
    <Characteristics>O</Characteristics>
    <Description>
      <p>Date on which purchase order was created.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>
  
  <Item>
    <Element>TaxRate</Element>    
    <Characteristics>O</Characteristics>
    <Description>
      <p>Rate of tax included in the billed amount.</p>
    </Description>
    <Type name="xs:decimal"></Type>
  </Item>
  
  
  <Item>
    <Element>TaxAmount</Element>   
    <Characteristics>O</Characteristics>
    <Description>
      <p>Amount of tax included in the billed amount.</p>
    </Description>
    <Type ref="CommonTypes" name="MonetaryAmountType"></Type>
  </Item>
  
  <Item>
    <Element>TotalAmount</Element>    
    <Characteristics>O</Characteristics>
    <Description>
      <p>Total amount billed.</p>
    </Description>
    <Type ref="CommonTypes" name="MonetaryAmountType"></Type>
  </Item>
  
  <Item>
    <Element>UpdateDate</Element>    
    <Characteristics>O</Characteristics>
    <Description>
      <p>Date on which the object was last updated.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>
  
  <Item>
    <Element>FullyDelivered</Element>    
    <Characteristics>O</Characteristics>
    <Description>
      <p>Flag that order has been fully delivered.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsYesOrNoCategoryType"></Type>
  </Item>
  
  <Item>
    <Element>OriginalPurchaseOrderRefId</Element>    
    <Characteristics>O</Characteristics>
    <Description>
      <p>Original purchase order that this purchase order updates (e.g. to cancel line items).</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  
  
  
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_MetadataType"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElementsType"></Type>
  </Item>
  <xhtml:Example xmlns="">
    <PurchaseOrder RefId="ED12345F-DA84-9727-5BC2-8AA349DD3721">
      <FormNumber>00342</FormNumber>
      <VendorInfoRefId>BD12345F-DA84-9727-5BC2-8AA349DD3723</VendorInfoRefId>
      <ChargedLocationInfoRefId>ED12345F-DA84-9727-5BC2-8AA349DD3722</ChargedLocationInfoRefId>
      <EmployeePersonalRefId>AD12345F-DA84-9727-5BC2-8AA349DD3721</EmployeePersonalRefId>
      <PurchasingItems>
        <PurchasingItem>
          <ItemNumber>154486</ItemNumber>
          <ItemDescription>Floor Wax</ItemDescription>
          <Quantity>10</Quantity>
          <UnitCost Currency="AUD">7.00</UnitCost>
          <ExpenseAccounts>
            <ExpenseAccount>
              <AccountCode>10-1100-610</AccountCode>
              <Amount Currency="AUD">70.00</Amount>
            </ExpenseAccount>
          </ExpenseAccounts>
        </PurchasingItem>
      </PurchasingItems> 
    </PurchaseOrder>
    
    
    
    

  </xhtml:Example>
</DataObject>