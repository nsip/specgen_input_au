<IncludeGroup
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xhtml="http://www.w3.org/1999/xhtml"
	
	xmlns="http://sifassociation.org/SpecGen">

	<!-- IncludeGroup is syntactic sugar that makes this a well-formed XML file that can
		 be validated separately; and allows the xi:include to have parse="xml"  SpecGen tool ignores it. -->


	<!-- Element types for the Wellbeing Domain -->

 <!-- Wellbeing Items-->   
 <CommonElement name="MedicationListType" type="true" dm="true">
	  <Item>
		<Element>MedicationListType</Element>
         <Characteristics>O</Characteristics>
		 <Description>
          <p>A list of Medications</p>
        </Description>
  		<List mode="List">
        </List>
    </Item>
    <Item>
      <Element>Medication</Element>
      <Characteristics>OR</Characteristics>
      <Description></Description>
	   <Type ref="CommonTypes" name="MedicationType"/>
	</Item>
 </CommonElement>
 <CommonElement name="MedicationType" type="true" dm="true">
    <Item>
		<Element>MedicationType</Element>
        <Characteristics>OR</Characteristics>
		 <Description>
          <p></p>
        </Description>
  	</Item> 
  	<Item>
  	   	<Element>MedicationName</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Name of medication required for the student.</p>
		</Description>
		<Type name="xs:string"></Type>  
  	</Item>
  	<Item>
	  	<Element>Dosage</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Dosage and strength of medication required for the student.</p>
		</Description>
		<Type name="xs:string"></Type>  
  	</Item>
  	<Item>
  	   	<Element>Frequency</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Frequency of Administration.</p>
		</Description>
		<Type name="xs:string"></Type>  
  	</Item>
  	<Item>
  	   	<Element>AdministrationInformation</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Information about how the medication is to be administered.</p>
		</Description>
		<Type name="xs:string"></Type>  
  	</Item>
  	<Item>
  	   	<Element>Method</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Method of administration.</p>
		</Description>
		<Type name="xs:string"></Type>  
  	</Item>
  </CommonElement>
  
 <CommonElement name="WellbeingEventCategoryListType" type="true" dm="true">
    <Item>
		<Element>WellbeingEventCategoryListType</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>List of Categories of WellbeingEvent.</p>
		</Description>
		<!-- Eventually to be a Code Set - needs to be further defined.
		<Type ref="CodeSets" name="AUCodeSetsWellbeingEventCategoryType"></Type>
		-->
		<List mode="List">
        </List>
	</Item>
	<Item>
      <Element>WellbeingEventCategory</Element>
      <Characteristics>MR</Characteristics>
      <Description></Description>
	  <Type ref="CommonTypes" name="WellbeingEventCategoryType"></Type>
	</Item>
  </CommonElement>
 <CommonElement name="WellbeingEventCategoryType" type="true" dm="true">
	 <Item>
		<Element>WellbeingEventCategoryType</Element>
        <Characteristics>MR</Characteristics>
		 <Description>
          <p>Category of event.</p>
        </Description>        	 
    </Item>
    <Item> 
 		<Element>EventCategory</Element>
        <Characteristics>M</Characteristics>
		 <Description>
          <p>Category of event.</p>
        </Description>
        <Union>
			<Type ref="CodeSets" name="AUCodeSetsEventCategoryType"/>
			<Type name="xs:string"/>
		</Union>	 
    </Item>	
	<Item>
		<Element>WellbeingEventSubCategoryList</Element>
		<Characteristics>C</Characteristics>
		<Description>
		  <p>List of sub-categories of the WellbeingEvent. Most likely to be locally defined.</p>
		</Description>
		 <Type ref="CommonTypes" name="WellbeingEventSubCategoryListType"></Type>
	 </Item>
 </CommonElement>
 
 <CommonElement name="WellbeingEventSubCategoryListType" type="true" dm="true">
	  <Item>
		<Element>WellbeingEventSubCategoryListType</Element>
         <Characteristics>O</Characteristics>
		 <Description>
          <p>List of SubTypes associated with a Category.</p>
        </Description>
  		<List mode="List">
        </List>
    </Item>
    <Item>
      <Element>WellbeingEventSubCategory</Element>
      <Characteristics>MR</Characteristics>
      <Description></Description>
	    <Union>
			<Type ref="CodeSets" name="AUCodeSetsEventSubCategoryType"/>
			<Type name="xs:string"/>
		</Union>	 
	</Item>
 </CommonElement>
 
 <CommonElement name="WellbeingEventLocationDetailsType" type="true" dm="true">
    <Item>
		<Element>WellbeingEventLocationDetailsType</Element>
		 <Description>
          <p>Container for the location details of a WellbeingEvent.</p>
        </Description>
  	</Item> 
  	<Item>
		<Element>EventLocation</Element>
		<Characteristics>M</Characteristics>
		<Description>
		  <p>Categorisation of location of Wellbeing event.</p>
		</Description>
		<Type ref="CodeSets" name="AUCodeSetsWellbeingEventLocationType"/>
	  </Item>
	  
	  <Item>
  	   	<Element>Class</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>If the event happened during a class, - Subject or Class the event occurred in. Free text for additional info if required.</p>
		</Description>
		<Type name="xs:string"></Type>  
  	 </Item>
  	 
	  <Item>
		<Element>FurtherLocationNotes</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>More information if required about the location.</p>
		</Description>
 		<Type name="xs:string"></Type>  
  </Item>
   
 </CommonElement>
 
 <CommonElement name="FollowUpActionListType" type="true" dm="true">
	  <Item>
		<Element>FollowUpActionListType</Element>
         <Characteristics>O</Characteristics>
		 <Description>
          <p>List of Actions where an action resulted and is known – maybe just a link to a WellbeingResponse Object</p>
        </Description>
  		<List mode="List">
        </List>
    </Item>
    <Item>
      <Element>FollowUpAction</Element>
      <Characteristics>OR</Characteristics>
      <Description>Container for the Follow Up details.</Description>
	   <Type ref="CommonTypes" name="FollowUpActionType"/>
	</Item>
 </CommonElement>
 
 <CommonElement name="FollowUpActionType" type="true" dm="true">
    <Item>
		<Element>FollowUpActionType</Element>
        <Characteristics>OR</Characteristics>
		 <Description>
          <p>Repeatable Follow Up Action.</p>
        </Description>
  	</Item> 
  	
  	<Item>
    <Element>WellbeingResponseRefId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The RefId of a WellbeingResponse Object if it exists.</p>
    </Description>
	<Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  
  	<Item>
  	   	<Element>FollowUpDetails</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Optional free text to outline follow up.</p>
		</Description>
		<Type name="xs:string"></Type>  
  	</Item>
  	
  	<Item>
  	   	<Element>FollowUpActionCategory</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Category of Action - typing locally defined.</p>
		</Description>
		<Type name="xs:string"></Type>  
  	</Item>
  
 </CommonElement>
 
 <CommonElement name="PersonInvolvementListType" type="true" dm="true">
	  <Item>
		<Element>PersonInvolvementListType</Element>
         <Characteristics>O</Characteristics>
		 <Description>
          <p>List of People involved in the Wellbeing event/response.  Mandatory if more people than the student to which this event/response occurs is involved.</p>
        </Description>
  		<List mode="List">
        </List>
    </Item>
    <Item>
      <Element>PersonInvolvement</Element>
      <Characteristics>OR</Characteristics>
      <Description>A person (additional to the student) involved in a  Wellbeing event/response where required to be known.  </Description>
	   <Type ref="CommonTypes" name="PersonInvolvementType"/>
	</Item>
 </CommonElement>
 <CommonElement name="PersonInvolvementType" type="true" dm="true">
    <Item>
		<Element>PersonInvolvementType</Element>
        <Characteristics>OR</Characteristics>
		 <Description>
          <p>A person (additional to the student) involved in a  Wellbeing event/response where required to be known.</p>
        </Description>
  	</Item> 
  	<Item>
  	   	<Element>PersonRefId</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>The RefId of the Person involved in the event, can be supplied if the Person is already a staff, student or student contact. </p>
		</Description>
		<Type ref="CommonTypes" name="IdRefType"></Type>
  	</Item>
  	  <Item>
    <Attribute>SIF_RefObject</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>
        The type of SIF object that the <code>PersonRefId</code> attribute identifies.
      </p>
    </Description>
    <Values>
      <Value>
        <Code>StudentPersonal</Code>
      </Value>
      <Value>
        <Code>StaffPersonal</Code>
      </Value>
      <Value>
        <Code>StudentContactPersonal</Code>
      </Value>
    </Values>
  </Item>
  
  <Item>
  	   	<Element>ShortName</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Optional free text name for a person, usually only used where the person does not have a personal record with a RefId.</p>
		</Description>
		<Type name="xs:string"></Type>  
  	</Item>
  	
  	<Item>
  	   	<Element>HowInvolved</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Optional free text detailing involvement.</p>
		</Description>
		<Type name="xs:string"></Type>  
  	</Item>
  	
 </CommonElement>
 
 <CommonElement name="WithdrawalTimeListType" type="true" dm="true">
	  <Item>
		<Element>WithdrawalTimeListType</Element>
         <Characteristics>O</Characteristics>
		 <Description>
          <p>Where a student is to be removed or withdrawn partially for particular times in a week within the Start and End dates of the Response.</p>
        </Description>
  		<List mode="List">
        </List>
    </Item>
    <Item>
      <Element>Withdrawal</Element>
      <Characteristics>MR</Characteristics>
      <Description></Description>
	   <Type ref="CommonTypes" name="WithdrawalType"/>
	</Item>
 </CommonElement>
 
 <CommonElement name="WithdrawalType" type="true" dm="true">
    <Item>
		<Element>WithdrawalType</Element>
        <Characteristics>MR</Characteristics>
		 <Description>
          <p>Repeatable Withdrawal.</p>
        </Description>
  	</Item> 
  	<Item>
  	   	<Element>WithdrawalDate</Element>
		<Characteristics>M</Characteristics>
		<Description>
		  <p></p>
		</Description>
		<Type name="xs:date"></Type>  
  	</Item>
  	<Item>
  	   	<Element>WithdrawalStartTime</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Start time of the Withdrawal on that day.</p>
		</Description>
		<Type name="xs:time"></Type>  
  	</Item>
  	<Item>
  	   	<Element>WithdrawalEndTime</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>End time of the Withdrawal on that day.</p>
		</Description>
		<Type name="xs:time"></Type>  
  	</Item>
  	<Item>
    <Element>TimeTableSubjectRefId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The GUID of an associated TimeTableSubject Object if there is one.</p>
    </Description>
