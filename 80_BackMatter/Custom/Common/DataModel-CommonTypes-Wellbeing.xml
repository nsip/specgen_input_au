 <CommonElement name="WellbeingDocumentType" type="true" dm="true">
    <Item>
		<Element>WellbeingDocumentType</Element>
        <Characteristics>MR</Characteristics>
		 <Description>
          <p>Repeatable information of the location of any associated documents kept in relation. These include evidentiary documents, and action plans.</p>
        </Description>
  	</Item>
  	<Item>
		<Element>Location</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Location of Documentation.</p>
		</Description>
		<Type name="xs:string"></Type>
   </Item> 
    <Item>
		<Element>Sensitivity</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Sensitivity of Document to give an indication of the audience who should have access.</p>
		</Description>
		<Type name="xs:string"></Type>
   </Item> 
  	<Item>
  	   	<Element>URL</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>URL if known.</p>
		</Description>
		<Type name="xs:anyURI"></Type>  
  	</Item>
  	<Item>
  	   	<Element>DocumentType</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Type of Document – could be a ‘Plan’.</p>
		</Description>
		<Type name="xs:normalizedString"></Type>  
  	</Item>
  	<Item>
  	   	<Element>DocumentReviewDate</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Document Review Date.</p>
		</Description>
		<Type name="xs:date"></Type>  
  	</Item>
	 <Item>
  	   	<Element>DocumentDescription</Element>
