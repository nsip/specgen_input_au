<!--   Scheduled Activity ie: not included in TimeTable -->
<CommonElement name="ScheduledActivityOverrideType" type="true" dm="true">
  <Item>
    <Element>ScheduledActivityOverrideType</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>This flag should be used to indicate whether or not this ScheduledActivity overrides a previous ScheduledActivity. In any initial provisioning this element should NOT be provided.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsYesOrNoCategoryType"></Type>
  </Item>
     
   <Item>
    <Attribute>DateOfOverride</Attribute>
    <Characteristics>C</Characteristics>
    <Description>
      <p>The date needs to be provided if Override=Yes.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item> 
  </CommonElement>
<CommonElement name="TeachingGroupListType" type="true" dm="true">
	<Item>
		<Element>TeachingGroupListType</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Teaching groups which are scheduled to attend the event. Not required for cyclical events, as this would be expected to be provided from TimeTableCell/TeachingGroup instead.</p>
		</Description>
		<List mode="List"/>
	</Item>
	<Item>
		<Element>TeachingGroupRefId</Element>
		<Characteristics>MR</Characteristics>
		<Description>
			<p>GUID from the TeachingGroup Object that identifies this TeachingGroup.</p>
		</Description>
		<Type ref="CommonTypes" relationship="" count="?:1" name="IdRefType"/>
	</Item>
</CommonElement>

<!--  Student Activity -->
