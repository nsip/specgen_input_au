<CommonElement name="NAPTestContentType" type="true" dm="true">
 	
    <Item>
		<Element>NAPTestContentType</Element>
		<Characteristics>M</Characteristics>
		 <Description>
          <p>Contents of an assessment assigned in the context of NAP, targeted at a particular domain and year level.</p>
        </Description>
  	</Item> 	
  	

  	<Item>
		<Element>NAPTestLocalId</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Local identifier of NAP test.</p>
		</Description>
		<Type ref="CommonTypes" name="LocalIdType"/>
  </Item>
  
    <Item>
    <Element>TestName</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Name of the NAP test.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>  
  <Item>
		<Element>TestLevel</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Values: 3,5,7,9. The test level that the student is registering for.</p>
		</Description>
		<Type ref="CommonTypes" name="YearLevelType"/>
  </Item>  
  <Item>
    <Element>TestType</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Type of NAP Test assigned in the session</p>
    </Description>
	<Type ref="CodeSets" name="AUCodeSetsNAPTestTypeType"/>
  </Item>
   
    <Item>
		<Element>Domain</Element>
		<Characteristics>M</Characteristics>
		<Description>Domain of the NAP test. Values: Writing, Reading, Language Conventions - Spelling, Language Conventions - Grammar and Punctuation, Numeracy.</Description>
		<Type ref="CodeSets" name="AUCodeSetsNAPTestDomainType"/>
   </Item>
 
    <Item>
		<Element>TestYear</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Year that testing took place, e.g. 2017.</p>
		</Description>
	    <Type ref="CommonTypes" name="SchoolYearType"/>
	</Item>
	
	<Item>
		<Element>StagesCount</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Number of branching stages in the test; 1 for a flat test.</p>
		</Description>
	    <Type name="xs:int"/>
	</Item>
	
    <Item>
		<Element>DomainBands</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p></p>
		</Description>
	    <Type ref="CommonTypes" name="DomainBandsContainerType"/>
	</Item>
	 
    <Item>
		<Element>DomainProficiency</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p></p>
		</Description>
	    <Type ref="CommonTypes" name="DomainProficiencyContainerType"/>
	</Item>
 </CommonElement>
