<IncludeGroup
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xhtml="http://www.w3.org/1999/xhtml"
	
	xmlns="http://sifassociation.org/SpecGen">

	<!-- IncludeGroup is syntactic sugar that makes this a well-formed XML file that can
		 be validated separately; and allows the xi:include to have parse="xml"  SpecGen tool ignores it. -->


	<!-- Element types for the NAPLAN  Domain -->  
<CommonElement name="NAPTestItemContentType" type="true" dm="true">
 	
    <Item>
		<Element>NAPTestItemContentType</Element>
		<Characteristics>M</Characteristics>
		 <Description>
          <p>Contents of a Test Item in the context of NAP.</p>
        </Description>
  	</Item> 
  	
  	<Item>
		<Element>NAPTestItemLocalId</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Local identifier of NAP test item.</p>
		</Description>
		<Type ref="CommonTypes" name="LocalIdType"/> 
  </Item>
  <Item>
		<Element>ItemName</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Name of NAP test item.</p>
		</Description>
		<Type name="xs:normalizedString"></Type>
  </Item>
    <Item>
    <Element>ItemType</Element>
    <Characteristics>M</Characteristics>
	<Description>
      <p>Response type for the test item.</p>
    </Description>
	<Type ref="CodeSets" name="AUCodeSetsNAPTestItemTypeType"/>
  </Item>
  <!--Item>
		<Element>Domain</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Domain descriptor for the test item.</p>
		</Description>
	    <Type ref="CodeSets" name="AUCodeSetsNAPTestDomainType"/>
	</Item-->
	<Item>
		<Element>Subdomain</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Subdomain descriptor for the test item.</p>
		</Description>
	    <!--Type ref="CodeSets" name="AUCodeSetsNAPTestSubDomainType"/-->
	    <Type name="xs:normalizedString"></Type>
	</Item>
	<Item>
		<Element>WritingGenre</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Genre of writing for the test item. Values: Narrative, Persuasive.</p>
		</Description>
	    <Type ref="CodeSets" name="AUCodeSetsNAPWritingGenreType"/>
	</Item>
	<Item>
		<Element>ItemDescriptor</Element>
		<Characteristics>M</Characteristics>
		<Description>
		  <p>Description of the test item.</p>
		</Description>
		<Type name="xs:normalizedString"></Type>
	</Item>  
	<Item>
		<Element>ReleasedStatus</Element>
		<Characteristics>M</Characteristics>
		<Description>
		  <p>Whether the test item has been publicly released.</p>
		</Description>
		<Type name="xs:boolean"></Type>
	</Item>  
	<Item>
		<Element>MarkingType</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Method used to mark the test item. Values: AS Auto Scoring, MM Manual Marking, AES Auto Essay Scoring.</p>
		</Description>
		<Type ref="CodeSets" name="AUCodeSetsNAPTestItemMarkingTypeType"/>
	</Item>
    <Item>
		<Element>MultipleChoiceOptionCount</Element>
		<Characteristics>C</Characteristics>
		<Description>
		  <p>For multiple choice and multiple choices questions, the number of questions.</p>
		</Description>
		<Type name="xs:int"></Type>
	</Item>  
  
    <Item>
		<Element>CorrectAnswer</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p>Τhe correct answer to the test item.</p>
		</Description>
		<Type name="xs:normalizedString"></Type>
	</Item> 
	
	<Item>
		<Element>MaximumScore</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Full marks allocated for correct response.</p>
		</Description>
		<Type name="xs:decimal"></Type>
   </Item>
   
	<Item>
		<Element>ItemDifficulty</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Item difficulty on same scale as Student Scaled Score.</p>
		</Description>
		<Type name="xs:decimal"></Type>
   </Item> 
   	<Item>
		<Element>ItemDifficultyLogit5</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Item difficulty in logits at 0.5 probability.</p>
		</Description>
		<Type name="xs:decimal"></Type>
   </Item> 
    <Item>
		<Element>ItemDifficultyLogit62</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Item difficulty in logits at 0.62 probability.</p>
		</Description>
		<Type name="xs:decimal"></Type>
   </Item> 
   <Item>
		<Element>ItemDifficultyLogit5SE</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Standard Error for item difficulty in logits at 0.5 probability.</p>
		</Description>
		<Type name="xs:decimal"></Type>
   </Item> 
   <Item>
		<Element>ItemDifficultyLogit62SE</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Standard Error for item difficulty in logits at 0.62 probability.</p>
		</Description>
		<Type name="xs:decimal"></Type>
   </Item> 

   	<Item>
		<Element>ItemProficiencyBand</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>The item proficiency band. Uses the same classification as Student Scaled Score.</p>
		</Description>
		<Type name="xs:integer"></Type>
   </Item> 
  	<Item>
		<Element>ItemProficiencyLevel</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>The item proficiency level. Uses the same classification as Student Scaled Score.</p>
		</Description>
		<Type name="xs:normalizedString"></Type>
   </Item> 
   
  	<Item>
		<Element>ExemplarURL</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>The publicly accessible location of an exemplar for the test item. Used if the test item itself has not been published.</p>
		</Description>
		<Type name="xs:anyURI"></Type>
   </Item> 
 
   <Item>
    <Element>ItemSubstitutedForList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>List of the RefId of the main item(s) for which this is a Substitute item.</p>
    </Description>
    <Type ref="CommonTypes" name="SubstituteItemListType"></Type>
  </Item>
  
  <Item>
		<Element>ContentDescriptionList</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p></p>
		</Description>
		<Type ref="CommonTypes" name="ContentDescriptionListType"></Type>
  </Item>
  
   <Item>
		<Element>StimulusList</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p></p>
		</Description>
		<Type ref="CommonTypes" name="StimulusListType"></Type>
  </Item>	
    	
	<Item>
		<Element>NAPWritingRubricList</Element>
		<Characteristics>O</Characteristics>
		<Description>
		  <p></p>
		</Description>
		<Type ref="CommonTypes" name="NAPWritingRubricListType"></Type>
  </Item>		
  	
  	</CommonElement>
<CommonElement name="NAPTestletContentType" type="true" dm="true">
 	
    <Item>
		<Element>NAPTestletContentType</Element>
		<Characteristics>M</Characteristics>
